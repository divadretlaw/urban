	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"comment.line.double-slash",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6C7986"
			}
		},
		{
			"name": "Documentation Markup",
			"scope": [
				"documentation",
				"comment.line.triple-slash.documentation"
			],
			"settings": {
				"foreground": "#6C7986"
			}
		},
		{
			"name": "Red",
			"scope": [
				"string",
				"meta.image.inline.markdown"
			],
			"settings": {
				"foreground": "#FC6A5D",
			}
		},
		{
			"name": "Purple",
			"scope": [
				"constant.numeric",
				"constant.character",
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#A4A0F1",
			}
		},
		{
			"name": "Magenta bold",
			"scope": [
				"keyword",
				"keyword.operator.new",
				"keyword.operator.type-casting",
				"meta.var",
				"storage.modifier.attribute",
				"storage.modifier",
				"storage.class",
				"storage.type",
				"variable.language.this",
				"variable.language.self",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#EF82B1",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Magenta normal",
			"scope": [
				"variable.language",
				"constant.language",
				"keyword.operator.logical.python",
				"markup.inline.raw",
				"markup.raw.block",
				"markup.fenced_code.block"
			],
			"settings": {
				"foreground": "#EF82B1",
			}
		},
		{
			"name": "Cyan",
			"scope": [
				"storage.modifier.import",
				"storage.modifier.package",
				"storage.type.java",
				"storage.type.generic",
				"storage.type.object",
				"meta.method.identifier",
				"meta.object.member",	
				"meta.struct",	
				"meta.class",
				"meta.inheritance-clause",
				"support.class",
				"support.function",
				"support.type",
				"support.variable",
				"variable.other",
				"meta.parameter-clause",
				"punctuation.definition.block",
				"markup.heading"
			],
			"settings": {
				"foreground": "#99E8D5",
			}
		},
		{
			"name": "Green",
			"scope": [
				"variable.other.object",
				"variable.other.object.property",
				"variable.other.property",
				"variable.object",
				"meta.function-call.generic",
				"markup.italic",
				"markup.bold"
			],
			"settings": {
				"foreground": "#ADD681",
			}
		},
		{
			"name": "White",
			"scope": [
				"punctuation.accessor",
				"punctuation.section",
				"punctuation.bracket",
				"punctuation.separator",
				"punctuation.defintion",
				"punctuation.definition.parameters",
				"punctuation.definition.typeparameters",
				"keyword.operator",
				"keyword.control.ternary",
				"variable.parameter",
				"variable.language.closure-parameter",
				"meta.brace",
				"meta.import",
				"meta.paragraph",
				"meta.method.body",
				"meta.definition.type",
				"storage.type.function.arrow",
				"entity.name.function",
				"entity.name.type",
				"variable.parameter.function",
				"support.variable.discard-value"
			],
			"settings": {
				"foreground": "#E7E8EB",
			}
		},
		{
			"name": "Orange",
			"scope": [
				"storage.type.annotation",
				"storage.type.function.arrow",
				"keyword.control.directive.include",
				"keyword.control.import.preprocessor.conditional",
				"meta.preprocessor",
				"meta.preprocessor.conditional",
				"support.constant",
				"markup.quote",
			],
			"settings": {
				"foreground": "#FD8F3F",
			}
		},
		{
			"name": "Blue",
			"scope": [
				"meta.link",
				"string.other.link.title"
			],
			"settings": {
				"foreground": "#74B6F6",
			}
		},
		{
			"name": "Yellow",
			"scope": [
				"markup.list"
			],
			"settings": {
				"foreground": "#FEFB85",
			}
		},
		{
			"name": "Dark-Gray",
			"scope": [
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#6C7986",
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"foreground": "#74B6F6",
				"fontStyle": "underline"
			}
		}
	  ]
}
